{
  "name": "豆瓣评分",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get pages source from a popup",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/douban.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "webNavigation",
    "http://localhost/"
  ]
}